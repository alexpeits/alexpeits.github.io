{
  "name": "alexpeits.github.io",
  "version": "1.0.0",
  "description": "alexpeits.github.io",
  "scripts": {
    "serve:watch": "browser-sync start --server _build --port 8000 --files _build/*/**",
    "check-links:all": "find . \\( -name '*.md' -or -name '*.yml' \\) -and \\( -path './posts/*' -or -path './pages/*' -or -path './README.md' \\) -exec markdown-link-check '{}' --config .github/config/markdown_link_check.json ';'",
    "check-links": "markdown-link-check --config .github/config/markdown_link_check.json",
    "md-lint": "markdownlint './posts/*.md' './pages/*.md' 'README.md'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexpeits/alexpeits.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexpeits/alexpeits.github.io/issues"
  },
  "homepage": "https://github.com/alexpeits/alexpeits.github.io#readme",
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "markdown-link-check": "^3.8.6",
    "markdownlint-cli": "^0.27.1"
  }
}
